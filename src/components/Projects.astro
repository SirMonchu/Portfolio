---
import ProjectCard from "./ProjectCard.astro";
import { t } from "i18next";
const PROJECTS = [
  {
    title: t("timeline.nowadays.title"),
    description:
      t("timeline.nowadays.desciption"),
    link: "https://app.pandafit.es/",
    imgSrc: "../layouts/pandafit.png",
  },
  {
    title: t("timeline.past.title"),
    description:
      t("timeline.past.desciption"),
    link: "https://cookieaware.vercel.app/login",
    imgSrc: "../layouts/cookieaware.png",
  },
]
---

<ol class="relative mt-16">
  {
    PROJECTS.map((projects) => (
      <li class="">
        <ProjectCard {...projects} />
      </li>
    ))
  }
</ol>