	<main class="px-4">
        <SectionContainer class="py-16 md:py-36">
            <h1 class="mb-4 text-4xl font-extrabold leading-none tracking-tight text-gray-900 md:text-5xl lg:text-6xl dark:text-white">Bienvenido!</h1>
            <AboutMe />
        </SectionContainer>
	</main>
